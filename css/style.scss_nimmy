.top-link {
    background: #1E3768;
    color: #ffffff;
    padding: 15px 0;
    font-size: 15px;
    font-weight: 700;

    .top-right-links {
        list-style: none;
        margin: 0;
        padding: 0;

        li {
            display: inline-block;
            color: #ffffff;

            a {
                color: #ffffff;
                text-decoration: none;
            }

            &:first-child {
                position: relative;
                padding-right: 10px;
                margin-right: 10px;

                &::after {
                    content: "|";
                    position: absolute;
                    font-size: 15px;
                    right: -5px;
                    top: 0;
                    font-weight: 400;
                }
            }

            &.social-links {
                margin-left: 30px;

