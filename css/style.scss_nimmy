.ref-lib-pdf-dwnd{
    border: 1px solid #1E3768;
    border-radius: 25px;
    -webkit-border-radius: 25px;
    -moz-border-radius: 25px;
    -ms-border-radius: 25px;
    -o-border-radius: 25px;
    padding: 30px;
    min-height: 100%;
    .title-sec{
        max-width: 80%;
        .hd-typ2{
            color: #1C4698;
            font-weight: 600;
            margin-bottom: 25px;
            text-decoration: none;
            &:hover{
                color: #EE7933;
            }
        }
        p{
            color: #000000;
            font-weight: 400;
            line-height: 1.2;
        }
    }
    .select-pdf{
        text-align: right;
        span{
            display: block;
            margin-top: 15px;
            background-color: #EE7933;
            color: #ffffff;
            font-size: 16px;
            font-weight: 500;
            border-radius: 5px;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            -ms-border-radius: 5px;
            -o-border-radius: 5px;
            padding: 2px 5px;
        }
        input[type=checkbox] {
            display: none;
            & ~ label {
                text-align: center;
                justify-content: center;
                align-items: center;
                color: #fff;
                background-color: #ffffff;
                border:2px solid #A39F9F;
                width: 25px;
                height: 25px;
                position: relative;
                &::after {
                    content: "";
                    display: none;
                    border-bottom: 2px solid #000000;
                    border-right: 2px solid #000000;
                    width: 7px;
                    height: 13px;
                    position: absolute;
                    left: 7px;
                    top: 2px;
                    transform: rotate(45deg);
                    -webkit-transform: rotate(45deg);
                    -moz-transform: rotate(45deg);
                    -ms-transform: rotate(45deg);
                    -o-transform: rotate(45deg);
                }
            }
            &:checked {
                & + label {
                    border-color: #000000;
                    &::after{
                        display: block;
                    }
                }
            }
        }
    }
    .library-post-info{
        span{
            font-size: 15px;
            font-weight: 300;
        }
    }
}
