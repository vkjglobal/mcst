.marquee-news{
    // transform: rotate(90deg);
    // display: flex;
    // height: 100%;
    // width: 100%;
    // padding: 0 60px;
    position: static;
    margin-bottom: 0;
    // height: 250px;
    .bx-controls{
        position: absolute;
        right: 0;
        top: 10px;
        z-index: 1;
        .bx-controls-direction{
            display: flex;

            a{
                position: static;
                &.bx-prev{
