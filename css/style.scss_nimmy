.logo-slider {
    padding: 0 50px;

    .img-wrp {
        display: flex;
        align-items: center;
        height: 120px;

        img {
            max-width: 100%;
        }
    }

    &.owl-theme {
        .owl-nav {
            position: absolute;
            width: 100%;
            left: 0;
            right: 0;
            top: 50%;
            display: flex;
            justify-content: space-between;
            align-items: center;
            height: 1px;
            margin-top: 0;

            button {
                span {
                    font-size: 0;
                    width: 25px;
                    height: 25px;
                    display: block;
                    border-left: 2px solid #CBCBCB;
                    border-bottom: 2px solid #CBCBCB;
                    transition: 0.3s;
                    -webkit-transition: 0.3s;
                    -moz-transition: 0.3s;
                    -ms-transition: 0.3s;
                    -o-transition: 0.3s;
                }

                &:hover {
                    background-color: transparent;

                    span {
                        border-color: #1E3768;
                        transition: 0.3s;
                        -webkit-transition: 0.3s;
                        -moz-transition: 0.3s;
                        -ms-transition: 0.3s;
                        -o-transition: 0.3s;
                    }
                }

                &.owl-prev {
                    span {
                        transform: rotate(45deg);
                        -webkit-transform: rotate(45deg);
                        -moz-transform: rotate(45deg);
                        -ms-transform: rotate(45deg);
                        -o-transform: rotate(45deg);
                    }
                }

                &.owl-next {
                    span {
                        transform: rotate(225deg);
                        -webkit-transform: rotate(225deg);
                        -moz-transform: rotate(225deg);
                        -ms-transform: rotate(225deg);
                        -o-transform: rotate(225deg);
                    }
                }
            }
        }
    }
}
