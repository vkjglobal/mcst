.banner-section {
    // margin-top: 40px;
    position: relative;
    color: #ffffff;

    .link-wrp {
        position: absolute;
        left: 0;
        right: 0;
        top: 50%;
        z-index: 9;
        height: 1px;
    }

    .home-banner {
        h2 {
            font-family: Inter;
            font-size: 25px;
            font-weight: 700;
            // color: #1E3768;
        }

        p {
            font-family: Inter;
            font-size: 18px;
            font-weight: 400;
            // color: #676767;
            display: -webkit-box;
            -webkit-line-clamp: 5; /* number of lines to show */
                    line-clamp: 5; 
            -webkit-box-orient: vertical;
            overflow: hidden;
        }

        .img-wrp {
            margin-right: 1px;
            min-height: 250px;
            background-repeat: no-repeat;
            background-size: cover;
            background-position: center;

